{
  "name": "nexus-browser",
  "version": "0.0.1",
  "author": "Nexus Concepts",
  "homepage": "https://nexusconcepts.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run packages && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "packages": "npm run build-packages || npm run write-packages",
    "build-packages": "npx tsc ./scripts/list-plugins.ts > /dev/null",
    "write-packages": "node ./scripts/list-plugins.js",
    "assets": "npx capacitor-assets generate --iconBackgroundColor '#333' --iconBackgroundColorDark '#333' --splashBackgroundColor '#333' --splashBackgroundColorDark '#333'"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^15.0.0",
