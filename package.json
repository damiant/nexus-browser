    "@capacitor/text-zoom": "4.1.0",
    "@capacitor/toast": "4.1.0",
    "@ionic/angular": "7.0.0-beta.1",
    "cordova-plugin-discover": "github:dtarnawsky/cordova-plugin-discover",
    "ionicons": "^6.0.3",
